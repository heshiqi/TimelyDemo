<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rootView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#011592"
    tools:context="com.android.testapplication.activity.MainActivity">

    <com.android.testapplication.widget.TimeTranslationView
        android:id="@+id/moveView"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="#33000000"
        />

    <com.android.testapplication.widget.TimeBarView
        android:id="@+id/timeBar"
        android:layout_width="200dp"
        android:layout_height="match_parent"
        />


</FrameLayout>
